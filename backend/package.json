{
  "name": "express-typescript-api",
  "version": "1.0.0",
  "description": "A simple task management API using Express and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start:json": "node dist/json-crud.js",
    "start:memory": "node dist/memory-crud.js",
    "start:postgresql": "node dist/postgresql-crud.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.44",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.12.0",
    "react-router": "^7.1.3",
    "react-router-dom": "^6.28.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.16",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "author": "Bee Vang",
  "license": "MIT"
}
